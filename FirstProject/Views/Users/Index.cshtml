@using MVCWebProject.ViewModels
@using MVCWebProject.ViewModels.Users
@model UsersListingDataViewModel
<div id="page-header">
    <h1>Users</h1>
</div>
<div id="page-wrap">
    <div class="listing-wrap">
        <div class="row">
            <div class="col-1-3">
                <a class="button" href="@Url.Action("Create")">Create</a>
            </div>
            <div class="col-1-3 right">
                <div class="search right">
                    @using (Ajax.BeginForm("Index", new AjaxOptions { UpdateTargetId = "Listing" }))
                    {
                        <input type="search" name="searchString" placeholder="Search for..." />
                        <input type="submit" value="Go" />
                    }
                </div>
            </div>
        </div>
        <div id="Listing">
            @Html.Partial("Listing", Model)
        </div>
    </div>
    <div id="modal"></div>
</div>
<div id="page-footer">
</div>